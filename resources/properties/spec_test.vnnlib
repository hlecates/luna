; VNNLIB property for spec_test.onnx
; Input dimension: 5
; Output dimension: 3
; Input bounds: [0.0, 1.0] for all inputs
; Output property: Y_0 >= 0.5

; Input variables
(declare-const X_0 Real)
(declare-const X_1 Real)
(declare-const X_2 Real)
(declare-const X_3 Real)
(declare-const X_4 Real)

; Output variables
(declare-const Y_0 Real)
(declare-const Y_1 Real)
(declare-const Y_2 Real)

; Input constraints: 0 <= X_i <= 1 for all inputs
(assert (>= X_0 0.0))
(assert (<= X_0 1.0))
(assert (>= X_1 0.0))
(assert (<= X_1 1.0))
(assert (>= X_2 0.0))
(assert (<= X_2 1.0))
(assert (>= X_3 0.0))
(assert (<= X_3 1.0))
(assert (>= X_4 0.0))
(assert (<= X_4 1.0))

; Output propertys
; (<= Y_0 Y_1) -> Y_0 <= Y_1, which normalizes to Y_0 - Y_1 <= 0
;(assert (<= Y_0 Y_1))

;(assert (<= Y_0 0.5))

(assert (or (>= Y_0 0.5) (<= Y_0 Y_1)))